<div class="common-header" ng-include="'js/apps/common/views/headerNav.html'"></div>

<div class="pro-wrapper my-scroll-pager" get-data-fn="pullData()" load-mark="loadMark">
	<div class="pro-list">
		<div class="pro-item" ng-repeat="item in dataList">
			<div class="section half-bottom-border">
				<i class="iconfont" ng-class='{true: "icon-chooses",false:"icon-choosen"}[!!store.checked]' ng-click="checkStore($index)"></i>
				<span class="pic half-border"><img bt-pic ng-model="item.goods_image_url" /></span>
				<div class="info-rect">
					<h4>{{item.goods_name}}</h4>
					<p class="price">Â¥ {{Util.toFixed(item.goods_price, 2)}}</p>				
					<i class="iconfont del" ng-click="delPro(item.cart_id)">&#xe605;</i>
				</div>
			</div>
		</div>
	</div>
</div>

