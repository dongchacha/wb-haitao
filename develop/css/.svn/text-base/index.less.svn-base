/*
 * 全站入口样式
 */  
@charset "UTF-8"; 
@import "variables.less";   
@import "mixin.less";
@import "base.less";
 
/********** 应用模块引入 begin ********/
// @import url('home.min.less');//首页 

// @import url('car.min.less');//车辆模块  
   
// @import url('store.min.less');//店铺  

/**
 * 公共顶部导航
 */
.common-header{
    position: fixed;
    z-index: 100;
    left: 0;
    right: 0;
    top: 0;
    height: @barBaseHeight;
    line-height: @barBaseHeight;
    background: #FFF url(../img/header-bg.png) repeat-x;
    // background: -webkit-gradient(linear, 50% 100%, 50% 0, from(#f5f5f5), to(#FFF));
    text-align: center;
    font-size: 16px;
    .icon-arrow-left{
        position: absolute;
        top: 0;
        left: 0;
        width: 40px;
        color: #ACACAC;
    }
    &+*{
        margin-top: @barBaseHeight;
    }
    .search-box{
	    position: absolute;
	    height: 30px;
	    border-radius: 3px;
	    background: rgba(3, 3, 3, 0.09);
	    left: 40px;
	    right: 15px;
	    top: 6px;
	    i{
		    position: absolute;
		    left: 10px;
		    top: 6px;
		    line-height: 1;
    		color: #7A797B;
	    }
    	input{
    		position: absolute;
    		font-size: 13px;
    		left: 0;
    		top: 0;
    		z-index: 1;
		    background: transparent;
		    display: block;
		    width: 100%;
			margin: 8px 0;
		    line-height: 1;
		    vertical-align: middle;
		    padding: 0 5px 0 30px;
		    border:0 none;
			color: transparent;
    	}

    	span{
		    position: relative;
		    z-index: 100;
		    float: left;
		    border-radius: 3px;
		    font-size: 12px;
		    line-height: 22px;
		    margin-top: 3px;
		    margin-left: 3px;
		    color: #fff;
		    padding: 0 6px;
		    background: #515151;
    		b{
    			margin-left: 5px;
    			font-size: 10px;
    			color: #fff;
    		}

    	}
    }
}

//广告模块
.adv-module{
    @marginHorizontal: 15px;
    @actionBtnColor: #ff7734; //按钮默认颜色
    .advs-container{
        position: relative;
        height: 250px;
        //默认图片
        .default-pic{
            width: 100%;
            height: 100%;
            background: url(../img/ad-default.png) center center no-repeat;
            background-size: 100%;
        }
        ul.pics{
            position: absolute;
            left: 0;
            top: 0;
            .ks-clear;
            li{
                background: #fff;
                float: left;
                a{
                	position: relative;
                    width: 100%;
                    height: 100%;
                    img{
                        max-width: 100%;
                        max-height: 100%;
                    }
                }
            }
        }
        ul.sort{
            position: absolute;
            bottom: 10px;
            left: 0;
            width: 100%;
            text-align: center;
            li{
                display: inline-block;
                width: 6px;
                height: 6px;
                border-radius: 6px;
                background: rgba(255,255,255, 0.5);
                &.curr{
                    background: rgba(255,255,255, 1);
                }
                &:nth-child(n+1){
                    margin-left: 10px;
                }
            }
        }
    }
    .actions{
        .display-box;
        padding: 15px @marginHorizontal;
        a{
            display: block;
            .box-flex(1);
            background: #ed4a44;
            font-size: 16px;
            color: #fff;
            border-radius: 5px;
            text-align: center;
            height: 40px;
            line-height: 40px;
            i{
                margin-left: 7px;
            }
            &:nth-child(n+2){
                margin-left: 15px;
                background: @actionBtnColor;
            }
        }
    }
}

//首页
.home-wrapper{
	position: relative;
    .adv-module .search-box{
		// background: #fff;
	    position: absolute;
		z-index: 2;
		left: 0;
		right: 0;
		top: 0;
	    height: 28px;
	    border-radius: 3px;
	    background: rgba(3, 3, 3, 0.09);
	    left: 15px;
	    right: 15px;
	    top: 15px;
	    i{
		    position: absolute;
		    z-index: 10;
		    left: 50%;
		    top: 4px;
		    margin-left: -55px;
		    line-height: 1;
    		color: #7A797B;
	    }
    	input{
    		position: absolute;
    		border-radius: 5px;
    		text-align: center;
    		left: 0;
    		top: 0;
    		z-index: 1;
		    background: #fff;
		    display: block;
		    width: 100%;
		    height: 100%;
		    /* width: 70%; */
		    // line-height: 1;
		    vertical-align: middle;
		    padding: 0 5px 0 25px;
		    border:0 none;
			color: transparent;
    	}
    }
	
	//品牌分类
    .category-box{
    	background: #fff;
    	padding: 10px 10px 0 10px;
    	a{
    		float: left;
    		width: 25%;
    		text-align: center;
    		padding: 5px 0 12px 0;
    		i{
    			color: #3C3C3C;
    			display: block;
				font-size: 40px;
				padding-bottom: 10px;
    		}
    		span{
    			display: block;
    			font-size: 12px;
    		}
    	}
    }
    section{
    	background: #fff;
	    h3{
	    	position: relative;
	    	color: #666;
	    	font-size: 20px;
	    	text-align: center;
    		line-height: 1.5;
    		padding: 15px 0 25px 0;
	    	&:after{
			    content: "";
			    position: absolute;
			    bottom: 15px;
			    left: 50%;
			    margin-left: -20px;
			    height: 2px;
			    width: 40px;
			    background: #DEDEDE;
	    	}
	    }
		
		//热门品牌
	    &.hot-brand{
	    	nav{
	    		a{
	    			background: #F0EFEF;
	    			height: 90px;
	    			width: 49%;
	    			width: calc(~"50% - 3px");
	    			width: -webkit-calc(~"50% - 3px");
	    			float: left;
	    			margin-bottom: 6px;
    				margin-right: 6px;
					-webkit-box-pack:center; 
					-moz-box-pack:center; 
					-webkit-box-align:center; 
					-moz-box-align:center; 
					overflow: hidden;
	    			&:nth-child(2n){
	    				margin-right: 0;
	    			}
	    			.display-box;
	    			.img-link{
	    				vertical-align: middle;
	    				width: 40%;
	    				padding: 5px;
	    				margin-right: 5px;
	    				img{
	    					max-width: 100%;
	    					max-height: 100%;
	    					margin: 0 auto;
	    				}
	    			}
	    			.info{
	    				// width: 55%;
	    				.box-flex(2);
	    				padding-right: 5px;
	    				p{
		    				overflow: hidden;
		    				text-overflow: ellipsis;
		    				white-space: nowrap;
	    				}
	    				.name{
	    					font-size: 16px;
	    					margin-bottom: 15px;
	    				}
	    				.desc{
	    					color: #999;
	    					font-size: 12px;
	    				}
	    			}
	    		}
	    	}
	    }

	    //精选活动
	    &.activities{
	    	.act-img-link{
	    		display: block;
	    		position: relative;
	    		img{
	    			width: 100%;
	    		}
	    		&:after{
				    content: "";
				    position: absolute;
				    left: 50%;
				    bottom: 0;
				    width: 0;
				    height: 0;
				    border-left: 8px solid transparent;
				    border-right: 8px solid transparent;
				    border-bottom: 8px solid #fff;
				    margin-left: -4px;
	    		}
	    	}
	    	.goods-box{
	    		width: 100%;
    			overflow-x: auto;
				-webkit-overflow-scrolling: touch;
				ul{
					li{
						width: 100px;
						height: 180px;
						float: left;
						padding: 10px 0px 0 10px;
						.img-link{
							position: relative;
							overflow: hidden;
							display: block;
							height: 90px;
							margin-bottom: 10px;
							img{
								display: block;
								margin: 0 auto;
								max-width: 100%;
								max-height: 100%;
							}
						}
						.name{
							color: #666;
							font-size: 12px;
						    line-height: 1.5;
						    height: 35px;
							display: block;
							overflow: hidden;
						    text-overflow: ellipsis;
						    -webkit-line-clamp: 2;
						    -webkit-box-orient: vertical;
							margin-bottom: 5px;
    						display: -webkit-box;
						}
						.price{
							text-align: center;
							font-size: 13px;
							i{
								margin-left: 10px;
								font-style: normal;
								font-size: 10px;
								color: #999;
								text-decoration: line-through;
							}
						}
					}
				}
	    	}
	    }

	    //人气推荐
	    &.recommeds{
	    	nav{
	    		.display-box;
	    		a{
	    			line-height: 44px;
	    			position: relative;
	    			display: block;
	    			min-width: 0;
	    			font-size: 14px;
	    			text-align: center;
	    			.box-flex(1);
	    			&.ks-cPrimary:after{
						content: "";
						width: 60px;
						height: 2px;
						position: absolute;
						bottom: 0;
						left: 50%;
						margin-left: -30px;
						background: @primaryColorRed;
	    			}
	    		}
	    	}
	    }
    }
}

//品牌活动
.brand-wrapper{
	.brand-intro{
		.bg-box{
			height: 180px;
		}
		.info-box{
			z-index: 2;
			background: #fff;
			position: relative;
			margin-bottom: 10px;
			.brand-img{
				position: absolute;
				left: 20px;
				top: -20px;
				width: 60px;
				height: 60px;
				&:after{
					z-index: 1;
				}
				img{
					width: 100%;
					height: 100%;
				}
			}
			h2{
				columns: #333;
				font-size: 16px;
				padding: 0 0 0 90px;
				line-height: 40px;
			}
			.desc{
				padding: 10px 0 5px 20px;
				font-size: 13px;
				p{
					padding-bottom: 5px;
					line-height: 1.5;
				}
			}
		}
	}
}

//活动
.activity-wrapper{
	nav{
		background: #fff;
		.display-box;
		a{
			line-height: 44px;
			position: relative;
			display: block;
			min-width: 0;
			font-size: 14px;
			text-align: center;
			.box-flex(1);
			&.ks-cPrimary:after{
				content: "";
				width: 60px;
				height: 2px;
				position: absolute;
				bottom: 0;
				left: 50%;
				margin-left: -30px;
				background: @primaryColorRed;
			}
		}
	}
}

//上传
.file-upload{
	font-size: 0;
	margin-right: -10px;
	a{
		position: relative;
		width:45px;
		height: 45px;
		display: inline-block;
		vertical-align: middle;
		margin-right: 10px;
		color: #999;
		// border:1px solid #ccc;
		text-align: center;
		line-height: 45px;
		&.pic-item{
			position: relative;
			span{
				border:1px solid #999;
				border-radius: 5px;
				display: block;
				width: 100%;
				height: 100%;
				overflow: hidden;
				border: 1px solid #ccc;
				img{
					display: block;
				}
			}
			.icon-shanchu{
				border-radius: 100px;
				position: absolute;
			    right: -7px;
			    top: -7px;
				font-size: 20px;
				width: 20px;
				height: 20px;
				line-height: 20px;
				z-index: 100;
				background: #fff;
			}
			//单选
			&.single{
				span{
					border-radius: 2px;
					border-width: 1px;
				}
				.icon-shanchu{
					display: none;
				}
			}
		}
		&.add{
			position: relative;
			// background: url(../img/add-pic.png) center center no-repeat;
			// background-size:  100% 100%;
			&:before{
				border-radius: 5px;
				position: absolute;
				left: 0;
				top: 0;
				right: 0;
				bottom: 0;
				z-index: 1;
				font-size: 45px;
				color: #ccc;
			}
		}
		input{
			position: absolute;
			left: 0;
			top: 0;
			right: 0;
			bottom: 0;
			opacity: 0;
			z-index: 10;
		}
	}
}

//登录注册
.form-wrapper{
	.gForm{
		margin: 15px 0 35px 0;
		background: #fff;
		.form-item{
			.display-box;
			color: @fontColorLightBlack;
			height: 44px;
			line-height: 44px;
			padding: 0 15px;
			font-size: 14px;
			&>span{
				display: block;
			}
			.left{
				min-width: 60px;
				white-space: pre;
			}
			.main{
				position: relative;
				.box-flex(1);
				input{
					font-size: inherit;
					display: block;
					width: 100%;
					background: transparent;
				    height: 30px;
				    line-height: 30px;
				    margin-top: 7px;
				}
				.getCode{
					position: absolute;
					right: 0;
					top: 0;
					color: @fontColorRed;
					&.forbid{
						color: @fontColorGray;
					}
				}
			}
		}
	}
	.other-login{
		margin: 50px 15px 0 15px;
		.title{
			border-bottom: 1px solid #ccc;
			span{
				display: block;
				color: @fontColorGray;
			    width: 110px;
			    text-align: center;
			    position: relative;
			    top: 7px;
				font-size: 14px;
				margin: 0 auto;
				background-color: @bgLightWhite;
				padding: 0 10px;
			}
		}
		.third-styles{
			margin-top: 30px;
			text-align: center;
			.iconfont{
				font-size: 50px;
				&.icon-weixin{
					color: #2FA825;
				}
			}
		}
	}
	.links{
		position: fixed;
		bottom: 50px;
		width: 100%;
		margin-top: 30px;
		text-align: center;
		a{
			color: @fontColorRed;
			.half-border(all, @fontColorRed, 5px);
			height: 35px;
			line-height: 35px;
			display: block;
			width: 100px;
			margin: 0 auto;
			font-size: 14px;
		}
	}
}

//登录
.login-wrapper{
	.form-wrapper .gForm{
		margin-bottom: 0 0 35px 0;
	}
	nav{
		@radius: 5px;
		height: 44px;
		.display-box;
		padding: 6px 10px;
		a{
			font-size: 14px;
			color: @primaryColorRed;
			border: 1px solid @primaryColorRed;
			&:first-child{
				border-top-left-radius: @radius;
				border-bottom-left-radius: @radius;
			}
			&:last-child{
				border-top-right-radius: @radius;
				border-bottom-right-radius: @radius;
			}
			// background: #fff;
			height: 30px;
			line-height: 30px;
			width: 0;
			.box-flex(1);
			text-align: center;
			&.curr{
				color: #fff;
				background-color: @primaryColorRed;
			}
		}
	}
}

//商品列表
.pro-list{
	.ks-clear;
	.pro-item{
		@padding: 10px;
		background: #fff;
		margin-bottom: @padding;
		color: @fontColorDeepBlack;
		header{
			.display-box;
			color: @fontColorLightBlack;
			padding: 0 @padding;
			height: @barTabHeight;
			line-height: @barTabHeight;
			font-size: 13px;
			>*{
				display: block;
			}
		    a{
		    	.box-flex(1);
			    position: relative;
			    top: -1px;
			    overflow: hidden;
			    text-overflow: ellipsis;
			    white-space: nowrap;
			    .icon-storen{
			    	font-size: 16px;
			    	position: relative;
			    	top: 1px;
			    	margin-right: 5px;
			    }
			    +span{
			    	margin-left: 10px;
			    }
		    }
		}
		.section{
			padding: @padding;
			// padding: @padding @padding @padding @padding * 2 + 120px;
			height: @padding * 2 + 80px;
			position: relative;
			.display-box;
			>*{
				display: block;
				height: 100%;
			}
			> .iconfont{
				position: relative;
				width: 28px;
				&:before{
					position: absolute;
					left: 50%;
					top: 50%;
					margin: -15px 0 0 -15px;
				}
			}
			.pic{
				display: block;
				// position: absolute;
				// left: @padding;
				// top: @padding;
				width: 100px;
				height: 80px;
				&:after{
					z-index: 1;
					border-radius: 3px;
					border-color: #ccc;
				}
				img{
					max-height: 100%;
					max-width: 100%;
					margin: 0 auto;
				}
			}
			.info-rect{
				position: relative;
				padding: 0 0 0 @padding;
				.fTo-bottom{
				    position: absolute;
				    bottom: 0;
				    left: @padding;
				    right: 0;
					.common-btn{
						margin-top: -10px;
					}
				}
				// .fixed-left-bottom{
				//     position: absolute;
				//     bottom: 0;
				//     left: @padding;
				// }
				// .fixed-right-bottom{
				//     position: absolute;
				//     bottom: 0;
				//     right: 0;
				// }
				.box-flex(1);
				.title{
   					.display-box;
					line-height: 1.3;
					margin-bottom: 7px;
					font-size: 14px;
				    span{
				    	display: block;
				    	&:nth-child(1){
							.box-flex(1);
						    overflow: hidden;
						    text-overflow: ellipsis;
						    white-space: nowrap;
				    	}
						&.price{
							position: relative;
							// margin-left: 10px;
							b{
								position: absolute;
								top: 22px;
								right: 0;
								color: #666;
								font-size: 14px;
								font-weight: normal;
							}
							// font-weight: bold;
						}
				    }
				}
				.desc{
					font-size: 12px;
   					margin-bottom: 5px;
					span{
						color: @fontColorLightBlack;
						margin-right: 10px;
						b{
							margin-left: 5px;
							font-weight: normal;
							color: @fontColorDeepBlack;
						}
					}
				}
				// .price{
				// 	// position: absolute;
				// 	// bottom: @padding;
				// 	// left: @padding * 2 + 120px;
				// 	color: @primaryColorRed;
				// 	font-size: 14px;
				// }
				.count-box{
					font-size: 0;
					>*{
						display: inline-block;
					}
					i{
						color: #979797;
						font-size: 24px;
					}
					span{
					    font-size: 15px;
					    position: relative;
					    top: -3px;
					    margin: 0 2px;
					    color: #666;
						min-width: 25px;
						padding: 0 3px;
						text-align: center;
					}
				}
				.del{
					bottom: 3px;
	    			font-size: 18px;
				}
			}
		}
		.rect{
			line-height: 44px;
			font-size:12px;
			padding: 0 @padding;
			&.stat-rect{
				text-align: right;
				strong{
					font-weight: normal;
					font-size: 16px;
				}
				.shipping-fee{
					padding-left: 10px;
				}
			}
			&.time-rect{
				text-align: right;
				color: #666;
				font-size: 12px;
			}
			&.action-rect{
				text-align: right;
				padding: 5px @padding;
				a{
					margin-left: 8px;
				}
			}
		}
		+#pullUp{
			margin: -8px 0 -5px 0;
		}
	}
	&.double-col{
		.pro-item{
			float: left;
			width: 50%;
			margin-bottom: 0;
			.section{
				padding: 10px 10px 0 10px;
				height: auto;
				display: block;
				.pic{
					width: auto;
					height: 150px;
				}
				.info-rect{
					padding:0;
					.title{
						display: block;
						span:first-child{
						    overflow: hidden;
						    text-overflow: ellipsis;
						    display: -webkit-box;
						    -webkit-line-clamp: 2;
						    -webkit-box-orient: vertical;
						    white-space: normal;
						    color: #666;
						    height: 40px;
						    font-size: 13px;
						    line-height: 1.5;
						}
						.price{
							text-align: center;
							i{
								margin-left:10px;
								color: #999;
								font-size:10px;
								font-style: normal;
    							text-decoration: line-through;
							}
						}
