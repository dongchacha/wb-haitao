			line-height: 22px;
			&:after{
				border-color: #666;
				border-radius: 5px;
			}
		}
	}

	.progress{
		.progress-item{
			@color: #D8D8D8;
			position: relative;
			font-size: 14px;
			padding: 5px 10px 5px 25px; 
			color: #999;
			line-height: 1.2;
			&:before{
				content: "";
				border-left: 1px solid @color;
				position: absolute;
				top: 12px;
				left: 7px;
				bottom: -10px;
			}
			&:after{
				content: "";
				position: absolute;
			    width: 10px;
			    height: 10px;
			    left: 3px;
			    top: 8px;
				border-radius: 100px;
				background-color: @color;
			}
			&:first-child{
				color: @primaryColorRed;
				&:after{
					top: 7px;
					left: 0;
					width: 10px;
					height: 10px;
					border:2px solid rgba(255, 64, 62, 0.3);
					background-color: @primaryColorRed;
    				background-clip: content-box;
				}
			}
			&:last-child:before{
				border: 0 none;
			}
			h4{

			}
			p{
				padding: 8px 0 12px 0;
				line-height: 1.5;
			}
		}
	}
}

