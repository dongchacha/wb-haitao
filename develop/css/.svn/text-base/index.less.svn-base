.search-wrapper{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 199;
    background-color: #fff;
    @paddingX: 15px;
    .scroll-box{
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: 1;
        background-color: #fff;
        overflow-y: scroll;
        -webkit-overflow-scrolling: touch;
    }
    //底部固定位置
    .fix-to-bottom{
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: 200;
        font-size: 15px;
        height: 40px;
        line-height: 40px;
        text-align: center;
        background: #fff;
        // .half-border(top, @splitLightGray);
        box-shadow: 0px -7px 7px -4px @splitLightGray;
      
        .ng-enter .fix-to-bottom, .ng-leave .fix-to-bottom{
            position: absolute !important;
            background-color: red !important;
        }   
    }
    //通用部分样式
    .common-section{
        padding-bottom: 15px;
        background: @bgWhite;
        ul{
            li{
                .half-border(all, @splitDarkGray, 3px, #fff);
                float: left;
                margin: 10px 10px 0 0;
                a{
                    display: block;
                    text-align: center;
                    color: @fontColorBlack;
                    height: 40px;
                    line-height: 40px;
                    min-width: 75px;
                }
                a.stress-item:active,&.curr a{
                    .half-border(all, #ffbdbd, 3px, #fff2f2);
                    color: @fontColorRed;
                }
            }
        }
    } 
    // -webkit-box-orient: vertical; 
    .data-list{
        position: relative;
        color: @fontColorDeepBlack;
        font-size: 16px;
        background-color: #fff;
        //margin-bottom: 40px;
        li{
            position: relative;
            padding: 0 15px;
            height: 50px;
            line-height: 50px;
            .ks-clear;
            a{
                padding: 0 3px;
                display: block;
                &:nth-child(n+1){
                    .half-border(bottom, @splitLightGray);
                }
            }
            i{
                position: absolute;
                right: 20px;
                top: 0;
                font-size: @fontSize14;
            }
            &.curr{
                a,i{
                    color: @primaryColorRed !important;
                }
            }
        }
    }
    .search-header{
        padding: 7px 12px 7px 12px;
        height: auto;
        // line-height: 30px;
        &:after{
            border-width:0 !important;
        }
        .search-box{
            margin: 0px 0px 0 0;
	    	// b.clear{
			   //  position: absolute;
			   //  right: 10px;
			   //  top: 3px;
			   //  z-index: 10;
			   //  height: 15px;
			   //  width: 15px;
			   //  background: url(../img/del-btn.png) no-repeat;
			   //  background-size: 15px auto;
	    	// }
        }
        .cancel{
            display: block;
            font-size: 15px;
            &.right{
            	line-height: 30px;
                margin-left: 10px;
            }
        }
    }
    //用于底部的站位
    .stance{
        height: 40px;
    }
    .slide-up-btn{
        i{
            color: #d1d1d1;
            margin-left: 5px;
        }
    }
    //关键词
    &.search-key{
        .search-history{
            top: 44px;
            header{
            	position: relative;
                padding: 15px 10px;
                font-size: 15px;
                // background: #f4f4f4;
                i.icon-detele{
				    position: absolute;
				    right: 12px;
				    top: 13px;
				    font-size: 17px;
                }
            }
            .key-list{
            	padding: 0 15px;
            	a{
            		color: #999;
            		float: left;
        			line-height: 27px;
        			margin:0 10px 10px 0;
        			padding: 0 10px;
            		&:after{
            			border-radius: 5px;
            		}
            	}
            }
            .clear-btn{
                display: block;
                width: 145px;
                margin: 15px auto;
                text-align: center;
                height: 45px;
                line-height: 45px;
                .half-border(all, @splitLightGray, 5px, #fff);
            }
        }
    }
    //综合
    &.search-multiple{
        .search-main{
        	position: absolute;
        	left: 0;
        	top: 0;
        	right: 0;
        	bottom: 44px;
        	background: #fff;
			font-size: 14px;
        	.display-box;
			>*{
				display: block;
			}
        	.col-class{
				height: 100%;
				overflow-y: scroll;
				-webkit-overflow-scrolling: touch;
				a,li{
					display: block;
					line-height: 44px;
					height: 44px;
					padding-left: 20px;
				}
        	}
			.category-col,ul{
				.col-class;
			}
			.category-col{
				width: 100px;
			}
			ul{
				.box-flex(1);
				li{
					position: relative;
					.icon-correct{
						color: @primaryColorRed;
						position: absolute;
						right: 15px;
						top: 0;
					}
					//价格
					&.defined{
						padding-right: 20px;
						.price-box{
							line-height: 30px;
							color: #999;
							.display-box;
							div{
								.box-flex(1);
								font-size: 12px;
								input{
									width: 100%;
									padding: 0 10px;
									background: transparent;
								}
								&:after{
									border-radius: 5px;
									border-color: #999;
								}
							}
							span{
								display: block;
								text-align: center;
								width: 25px;
							}
						}
					}
				}
			}
        }
        //空站位
        @height: 44px;
        .stance{
            height: @height;
        }
        .fix-to-bottom{
            height: @height;
            line-height: @height;
            font-size: 16px;
            .display-box;
            span, a{
                display: block;
            }
            span{
                .box-flex(1);
            }
            a{
                width: 100px;
                color: #fff;
                font-weight: @fontWeight;
                background-color: @primaryColorRed;
                &.reset{
                	background-color: #F2F2F2;
                	color: #666;
                }
            }
        }
    }
    
    //通用底部固定操作栏
    .fixd-bottom{
        position: fixed;
        left: 0;
        right: 0;
        bottom: 0;
        z-index: 999;
        padding: 10px 15px;
        box-shadow: 0px 0px 7px 3px @splitLightGray;
        background: #f8f8f8;
        .btn{
            display: block;
            border-radius: 5px;
            text-align: center;
            background: @fontColorRed;
            color: #fff;
            font-size: @fontSize16;
            height: 50px;
            line-height: 50px;
        }   
    }
}

