/*
 * 正文字体。
 */
html {
  background: transparent;
}
body,
button,
input,
select,
textarea {
  color: #333;
  /*font-family: "Microsoft Yahei", 宋体;*/
  font-family: "Helvetica Neue";
  word-break: break-all;
  word-wrap: break-word;
  border: 0 none;
}
::-webkit-scrollbar {
  display: none;
}
ol,
li,
fieldset,
h1,
h2,
h3,
h4,
img,
p,
input,
textarea {
  font-weight: normal;
  margin: 0;
  padding: 0;
  outline: medium none;
  border: 0 none;
  display: block;
}
body {
  line-height: 1;
  margin: 0;
  padding: 0;
  background-color: #EFEFF4;
  word-wrap: normal;
  width: 100%;
  overflow-x: hidden;
}
img {
