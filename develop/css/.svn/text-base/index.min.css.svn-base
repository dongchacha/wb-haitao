.overlay {
  background: #fff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1001;
}
.overlay header {
  text-align: center;
  line-height: 44px;
  font-size: 16px;
}
.overlay .line {
  display: box;
  display: -webkit-box;
  display: -ms-flexbox;
  display: -o-box;
  -webkit-box-orient: horizontal;
  -o-box-orient: horizontal;
  box-orient: horizontal;
  font-size: 14px;
  padding: 10px 15px;
  line-height: 1.8;
}
.overlay .line > span {
  display: block;
  width: 70%;
}
.overlay .line > span.name {
  width: 30%;
}
.overlay .line > span.val {
  color: #666;
}
.overlay .icon-close {
  position: absolute;
  right: 12px;
  top: 14px;
  z-index: 100;
}
.overlay .action-container {
  padding: 10px 0;
  display: box;
  display: -webkit-box;
  display: -ms-flexbox;
  display: -o-box;
  -webkit-box-orient: horizontal;
  -o-box-orient: horizontal;
  box-orient: horizontal;
}
.overlay .action-container .action-btn {
  display: block;
  box-flex: 1;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -ms-flex: 1;
  -o-box-flex: 1;
  font-size: 15px;
  height: 40px;
  line-height: 40px;
  border-radius: 5px;
  background: #FF403E;
  text-align: center;
  color: #fff;
  margin-top: 0;
}
.overlay .action-container .action-btn.addToCart-btn {
  background: #fff;
  color: #FF403E;
}
.overlay .action-container .action-btn.addToCart-btn:active {
  background-color: #fff !important;
}
.overlay.pro-spec .pro-info {
  height: 100px;
  position: relative;
  padding: 15px 0 0 130px;
}
.overlay.pro-spec .pro-info .pic-box {
  background: #fff;
  width: 100px;
  height: 100px;
  position: absolute;
  left: 15px;
  top: -15px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  overflow: hidden;
}
.overlay.pro-spec .pro-info .pic-box:after {
  border-radius: 5px;
  z-index: 1;
}
.overlay.pro-spec .pro-info .price {
  font-size: 18px;
  margin-bottom: 10px;
  color: #FF403E;
}
.overlay.pro-spec .pro-info .storage {
  font-size: 12px;
  color: #666;
}
.overlay.pro-spec .line {
  position: relative;
  display: block;
  margin: 0 15px;
  padding-left: 0;
  padding-right: 0;
}
.overlay.pro-spec .line .title {
  color: #666;
}
.overlay.pro-spec .line > ul {
  *zoom: 1;
  margin: 5px 0 2px 0;
}
.overlay.pro-spec .line > ul:after {
  content: '\20';
  display: block;
  height: 0;
  clear: both;
}
.overlay.pro-spec .line > ul:after {
  content: '\20';
  display: block;
  height: 0;
  clear: both;
}
.overlay.pro-spec .line > ul li {
  height: 27px;
  line-height: 27px;
  background: #EDEDED;
  color: #999;
  padding: 0 15px;
  float: left;
  border-radius: 50px;
  margin-right: 20px;
  margin-bottom: 10px;
}
.overlay.pro-spec .line > ul li.curr {
  background: #FF403E;
  color: #fff;
}
.overlay.pro-spec .line .count-box {
