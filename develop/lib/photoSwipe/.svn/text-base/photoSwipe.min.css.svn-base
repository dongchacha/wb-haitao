.photoSwipe-layer{position:fixed;left:0;top:0;z-index:9998;background:#000;width:100%;overflow:hidden;font-size:0}.photoSwipe-layer .item-list{position:absolute;top:0;list-style:none;padding:0;display:table}.photoSwipe-layer .item-list ul{width:100%;height:100%;display:table-row}.photoSwipe-layer .item-list ul li{height:100%;float:left;display:block;vertical-align:top;position:relative;list-style:none;text-align:center;overflow:scroll;-webkit-overflow-scrolling:touch}.photoSwipe-layer .item-list ul li img{-webkit-transition:-webkit-transform .2s linear,-webkit-transform .2s linear,width .2s linear,height .2s linear,max-width .2s linear,max-height .2s linear;transition:transform .2s linear,-webkit-transform .2s linear,width .2s linear,height .2s linear,max-width .2s linear,max-height .2s linear;max-width:100%;max-height:100%;margin:0 auto;border:0;display:block}.photoSwipe-layer .icon-list{position:absolute;font-size:20px;color:#fff}.photoSwipe-layer .icon-list i{display:inline-block;width:8px;height:8px;border-radius:8px;background:#FFF;margin-right:5px}.photoSwipe-layer .icon-list i.current{background:#484343}.photoSwiper-tmpContainer{position:fixed;z-index:9999;left:0;top:0;right:0;bottom:0;background:#000}.photoSwiper-tmpContainer img{position:absolute}