/**
 * 页面进入进出动画
 */
.an-delay.ng-enter {
  -webkit-animation-delay: .4s !important;
  animation-delay: .4s !important;
}
.an-slideLeft {
  /* 从左侧移入 */
  /* 向左侧移出 */
}
.an-slideLeft.ng-enter {
  z-index: 100;
  -webkit-animation: kf-enter 0.4s both ease;
  -webkit-animation-delay: 0.1s;
  animation-delay: 0.1s;
}
.an-slideLeft.ng-leave {
  z-index: 100;
  -webkit-animation: kf-leave 0.4s both ease;
}
@-webkit-keyframes kf-enter {
  0% {
    -webkit-transform: translate3D(-100%, 0, 0);
    transform: translate3D(-100%, 0, 0);
  }
  5% {
    -webkit-transform: translate3D(-100%, 0, 0);
    transform: translate3D(-100%, 0, 0);
  }
  100% {
    -webkit-transform: translate3D(0, 0, 0);
    transform: translate3D(0, 0, 0);
  }
}
@-webkit-keyframes kf-leave {
  0% {
    -webkit-transform: translate3D(0, 0, 0);
    transform: translate3D(0, 0, 0);
  }
  100% {
    -webkit-transform: translate3D(-100%, 0, 0);
    transform: translate3D(-100%, 0, 0);
  }
}
.an-slideRight {
  /* 从右侧进入 */
  /* 从右侧移出 */
}
.an-slideRight.ng-enter {
  z-index: 200;
  -webkit-animation: slideRight-enter 0.4s both ease;
}
.an-slideRight.ng-leave {
  z-index: 200;
  -webkit-animation: slideRight-leave 0.4s both ease;
}
@-webkit-keyframes slideRight-enter {
  0% {
    -webkit-transform: translate3D(100%, 0, 0);
    transform: translate3D(100%, 0, 0);
  }
  5% {
    -webkit-transform: translate3D(100%, 0, 0);
    transform: translate3D(100%, 0, 0);
  }
  100% {
    -webkit-transform: translate3D(0, 0, 0);
    transform: translate3D(0, 0, 0);
  }
}
@-webkit-keyframes slideRight-leave {
  from {
    -webkit-transform: translate3D(0, 0, 0);
    transform: translate3D(0, 0, 0);
  }
  to {
    -webkit-transform: translate3D(100%, 0, 0);
    transform: translate3D(100%, 0, 0);
  }
}
.an-slideUp {
  /* 从上面下来 */
  /* 从上面移出 */
}
.an-slideUp.ng-enter {
  z-index: 200;
  -webkit-animation: slideUp-enter 0.3s both ease;
  -webkit-animation-delay: 0.1s;
  animation-delay: 0.1s;
}
.an-slideUp.ng-leave {
  z-index: 200;
  -webkit-animation: slideUp-leave 0.3s both ease;
}
@-webkit-keyframes slideUp-enter {
  0% {
    -webkit-transform: translateY(-100%);
    transform: translateY(-100%);
  }
  5% {
    -webkit-transform: translateY(-100%);
    transform: translateY(-100%);
  }
  100% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}
@-webkit-keyframes slideUp-leave {
  from {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
  to {
    -webkit-transform: translateY(-100%);
    transform: translateY(-100%);
  }
}
.an-slideDown {
  /* 从底下上来 */
  /* 从底下移出 */
}
.an-slideDown.ng-enter {
  z-index: 200;
  -webkit-animation: slideDown-enter 0.3s both ease;
}
.an-slideDown.ng-leave {
  z-index: 200;
  -webkit-animation: slideDown-leave 0.3s both ease;
}
@-webkit-keyframes slideDown-enter {
  0% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
  5% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
  100% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}
@-webkit-keyframes slideDown-leave {
  from {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
  to {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
}
.an-fadeInOut {
  /* 从底下上来 */
  /* 从底下移出 */
}
.an-fadeInOut.ng-enter {
  z-index: 200;
  -webkit-animation: fadeInOut-enter 0.3s both ease;
}
.an-fadeInOut.ng-leave {
  z-index: 200;
  -webkit-animation: fadeInOut-leave 0.3s both ease;
}
@-webkit-keyframes fadeInOut-enter {
  0% {
    opacity: 0;
    filter: alpha(opacity=0);
  }
  5% {
    opacity: 0;
    filter: alpha(opacity=0);
  }
  100% {
    opacity: 1;
    filter: alpha(opacity=100);
  }
}
@-webkit-keyframes fadeInOut-leave {
  from {
    opacity: 1;
    filter: alpha(opacity=100);
  }
  to {
    opacity: 0;
    filter: alpha(opacity=0);
  }
}
:hover .arrow-rotate,
:hover .arrow-rotate-back {
  will-change: transform;
}
.arrow-rotate {
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-animation: kf-arrow-asc 0.2s linear forwards;
          animation: kf-arrow-asc 0.2s linear forwards;
}
@-webkit-keyframes kf-arrow-asc {
  from {
    -webkit-transform: rotate(0);
    transform: rotate(0);
  }
  to {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
  }
}
.arrow-rotate-back {
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-animation: kf-arrow-desc 0.2s linear forwards !important;
          animation: kf-arrow-desc 0.2s linear forwards !important;
}
@-webkit-keyframes kf-arrow-desc {
  from {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
  }
  to {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
